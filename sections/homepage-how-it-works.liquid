138 45 L138 55" stroke="#FFF" stroke-width="1.5"/><circle cx="160" cy="22" r="2" fill="#F6E05E"/><circle cx="170" cy="35" r="1.5" fill="#F6E05E"/><circle cx="180" cy="25" r="1" fill="#F6E05E"/></svg>`;document.body.appendChild(sleigh);for(let i=0;i<40;i++){setTimeout(()=>{const snow=document.createElement('div');snow.className='bbh-sleigh-snow';snow.textContent='❄';snow.style.left=(5+Math.random()*90)+'%';snow.style.top=(5+Math.random()*20)+'%';document.body.appendChild(snow);setTimeout(()=>snow.remove(),2600)},i*80)}setTimeout(()=>sleigh.remove(),4200)}
function openTutorialVideo(url){const m=document.getElementById('bbhVideoModal'),w=document.getElementById('bbhVideoWrapper');if(m&&w){createSleighEffect();w.innerHTML='<iframe src="'+url+'?autoplay=1" allow="accelerometer;autoplay;clipboard-write;encrypted-media;gyroscope;picture-in-picture" allowfullscreen></iframe>';m.classList.add('active');document.body.style.overflow='hidden'}}
function closeTutorialVideo(){const m=document.getElementById('bbhVideoModal'),w=document.getElementById('bbhVideoWrapper');if(m&&w){w.innerHTML='';m.classList.remove('active');document.body.style.overflow=''}}
document.addEventListener('keydown',function(e){if(e.key==='Escape')closeTutorialVideo()});
document.getElementById('bbhVideoModal')?.addEventListener('click',function(e){if(e.target===this)closeTutorialVideo()});
</script>
