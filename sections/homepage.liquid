.coming-soon-overlay{position:absolute;top:0;left:0;right:0;bottom:0;background:linear-gradient(180deg,rgba(0,0,0,0.2) 0%,rgba(0,0,0,0.6) 100%);display:flex;align-items:center;justify-content:center;pointer-events:none}
