.coming-soon-label{position:absolute;left:10px;bottom:10px;display:flex;align-items:center;gap:6px;background:rgba(0,0,0,.7);backdrop-filter:blur(12px);color:rgba(255,255,255,0.9);font-weight:700;padding:8px 12px;border-radius:10px;border:1px solid rgba(255,255,255,0.15);font-size:12px;box-shadow:0 4px 12px rgba(0,0,0,.3);white-space:nowrap}

.modal-footer{padding:22px 0 0;text-align:center;border-top:2px solid #DBEAFE;margin-top:24px;position:relative;z-index:2}
.modal-footer-text{color:#3B82F6;font-size:17px;font-weight:800;text-transform:uppercase;letter-spacing:1px}

