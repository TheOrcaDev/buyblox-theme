/* Games Grid */
.modal-grid{display:grid;grid-template-columns:repeat(4,1fr);gap:16px;position:relative;z-index:2}
.game-option{position:relative;display:block;border-radius:18px;overflow:hidden;background:#FFF;box-shadow:0 4px 16px rgba(17,17,17,.08);border:3px solid #E2E8F0;color:#111;text-decoration:none!important;transition:all .3s ease}
