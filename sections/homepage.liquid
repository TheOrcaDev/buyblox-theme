.game-label-icon.fish{width:32px}
.game-label-icon.adopt{width:28px;height:28px;border-radius:6px}

/* Coming Soon Section */
.coming-soon-grid{display:grid;grid-template-columns:repeat(3,1fr);gap:16px}
.coming-soon-card{position:relative;display:block;border-radius:18px;overflow:hidden;background:#252538;box-shadow:0 4px 20px rgba(0,0,0,.3);border:3px solid #3a3a52;color:#111;text-decoration:none!important;transition:all .4s ease;cursor:default}
