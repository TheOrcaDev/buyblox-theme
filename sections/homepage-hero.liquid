<style>
.bbx-hero *{font-family:'Outfit',ui-sans-serif,system-ui,sans-serif!important}
.bbx-hero{position:relative;width:100vw;left:50%;margin-left:-50vw;padding:0;background:linear-gradient(180deg,#FFFFFF 0%,#F0F7FF 10%,#E0EFFF 30%,#D4E8FF 50%,#C8E2FF 70%,#E8F4FF 85%,#FFFFFF 100%);overflow:hidden;min-height:100vh}

/* ===== AURORA / NORTHERN LIGHTS ===== */
.bbx-aurora{position:absolute;top:0;left:0;width:100%;height:60%;pointer-events:none;z-index:1;overflow:hidden;opacity:0.4}
.bbx-aurora-wave{position:absolute;width:200%;height:300px;left:-50%;filter:blur(60px);animation:aurora-drift 15s ease-in-out infinite}
.bbx-aurora-wave:nth-child(1){top:5%;background:linear-gradient(90deg,transparent 0%,rgba(96,165,250,.3) 20%,rgba(147,197,253,.4) 40%,rgba(59,130,246,.3) 60%,transparent 80%);animation-delay:0s}
.bbx-aurora-wave:nth-child(2){top:15%;background:linear-gradient(90deg,transparent 10%,rgba(139,92,246,.2) 30%,rgba(96,165,250,.3) 50%,rgba(147,197,253,.2) 70%,transparent 90%);animation-delay:-5s;animation-duration:18s}
.bbx-aurora-wave:nth-child(3){top:8%;background:linear-gradient(90deg,transparent 5%,rgba(59,130,246,.2) 25%,rgba(96,165,250,.25) 45%,transparent 65%);animation-delay:-10s;animation-duration:20s}
@keyframes aurora-drift{0%,100%{transform:translateX(-10%) scaleY(1)}50%{transform:translateX(10%) scaleY(1.2)}}

/* ===== FLOATING PARTICLES / SPARKLES ===== */
.bbx-particles{position:absolute;top:0;left:0;width:100%;height:100%;pointer-events:none;z-index:2;overflow:hidden}
.bbx-particle{position:absolute;width:4px;height:4px;background:radial-gradient(circle,rgba(255,255,255,.9) 0%,rgba(147,197,253,.6) 50%,transparent 100%);border-radius:50%;animation:particle-float linear infinite}
.bbx-particle:nth-child(1){left:10%;top:20%;animation-duration:20s;animation-delay:0s}
.bbx-particle:nth-child(2){left:20%;top:60%;animation-duration:25s;animation-delay:-5s;width:3px;height:3px}
.bbx-particle:nth-child(3){left:30%;top:40%;animation-duration:22s;animation-delay:-10s}
.bbx-particle:nth-child(4){left:45%;top:70%;animation-duration:18s;animation-delay:-3s;width:5px;height:5px}
.bbx-particle:nth-child(5){left:55%;top:30%;animation-duration:24s;animation-delay:-8s}
.bbx-particle:nth-child(6){left:65%;top:55%;animation-duration:21s;animation-delay:-12s;width:3px;height:3px}
.bbx-particle:nth-child(7){left:75%;top:25%;animation-duration:19s;animation-delay:-2s}
.bbx-particle:nth-child(8){left:85%;top:65%;animation-duration:23s;animation-delay:-7s;width:4px;height:4px}
.bbx-particle:nth-child(9){left:5%;top:45%;animation-duration:26s;animation-delay:-15s;width:3px;height:3px}
.bbx-particle:nth-child(10){left:92%;top:35%;animation-duration:20s;animation-delay:-4s}
.bbx-particle:nth-child(11){left:38%;top:15%;animation-duration:22s;animation-delay:-9s;width:5px;height:5px}
.bbx-particle:nth-child(12){left:62%;top:80%;animation-duration:17s;animation-delay:-1s}
@keyframes particle-float{0%{transform:translateY(0) translateX(0) scale(1);opacity:0}10%{opacity:1}50%{transform:translateY(-100px) translateX(30px) scale(1.2);opacity:0.8}90%{opacity:0.6}100%{transform:translateY(-200px) translateX(-20px) scale(0.8);opacity:0}}

/* ===== BACKGROUND WINTER ELEMENTS ===== */
.bbx-hero-decor{position:absolute;top:0;left:0;width:100%;height:100%;pointer-events:none;z-index:3;overflow:hidden}

/* Ice Crystals */
.bbx-crystal{position:absolute;top:0;width:2px;background:linear-gradient(180deg,#93C5FD 0%,#BFDBFE 100%);z-index:3}
.bbx-crystal::after{content:'‚úß';position:absolute;bottom:-28px;left:50%;transform:translateX(-50%);font-size:24px;color:#60A5FA;text-shadow:0 0 10px rgba(96,165,250,.6);animation:crystal-spin 8s linear infinite}
.bbx-crystal:nth-child(1){left:8%;height:50px}.bbx-crystal:nth-child(1)::after{animation-delay:0s}
.bbx-crystal:nth-child(2){left:15%;height:70px}.bbx-crystal:nth-child(2)::after{content:'‚ùÑ';font-size:20px;animation-delay:2s}
.bbx-crystal:nth-child(3){right:15%;height:60px}.bbx-crystal:nth-child(3)::after{animation-delay:4s}
.bbx-crystal:nth-child(4){right:8%;height:45px}.bbx-crystal:nth-child(4)::after{content:'‚ùÑ';font-size:18px;animation-delay:6s}
@keyframes crystal-spin{0%{transform:translateX(-50%) rotate(0deg)}100%{transform:translateX(-50%) rotate(360deg)}}

/* Winter Glow Orbs */
.bbx-winter-glow{position:absolute;border-radius:50%;background:radial-gradient(circle,rgba(147,197,253,.25) 0%,transparent 70%);pointer-events:none;z-index:2;animation:glow-pulse 8s ease-in-out infinite}
.bbx-winter-glow:nth-child(5){width:350px;height:350px;top:5%;left:3%;animation-delay:0s}
.bbx-winter-glow:nth-child(6){width:450px;height:450px;top:15%;right:-5%;animation-delay:-3s}
.bbx-winter-glow:nth-child(7){width:300px;height:300px;bottom:40%;left:10%;animation-delay:-6s}
.bbx-winter-glow:nth-child(8){width:280px;height:280px;bottom:25%;right:8%;animation-delay:-2s}
@keyframes glow-pulse{0%,100%{opacity:0.5;transform:scale(1)}50%{opacity:0.8;transform:scale(1.1)}}

/* ===== SCATTERED WINTER DECORATIONS ===== */
.bbx-winter-decor{position:absolute;pointer-events:none;z-index:4;opacity:0.15;font-size:40px}
.bbx-winter-decor:nth-child(9){left:3%;bottom:35%;transform:rotate(-15deg)}
.bbx-winter-decor:nth-child(10){right:5%;bottom:45%;transform:rotate(10deg);font-size:35px}
.bbx-winter-decor:nth-child(11){left:8%;bottom:55%;font-size:25px;transform:rotate(5deg)}
.bbx-winter-decor:nth-child(12){right:12%;bottom:60%;font-size:30px;transform:rotate(-8deg)}

/* ===== PARALLAX SNOW LAYERS ===== */
.bbx-snow-layer{position:absolute;bottom:0;left:0;width:100%;pointer-events:none}
.bbx-snow-layer-back{height:280px;z-index:4;opacity:0.4}
.bbx-snow-layer-back svg{position:absolute;bottom:0;left:0;width:100%;height:100%}
.bbx-snow-layer-mid{height:220px;z-index:5;opacity:0.7}
.bbx-snow-layer-mid svg{position:absolute;bottom:0;left:0;width:100%;height:100%}
.bbx-snow-layer-front{height:160px;z-index:6}
.bbx-snow-layer-front svg{position:absolute;bottom:0;left:0;width:100%;height:100%}

/* ===== GRADIENT FADE TO WHITE ===== */
.bbx-gradient-fade{position:absolute;bottom:0;left:0;width:100%;height:350px;background:linear-gradient(180deg,transparent 0%,rgba(255,255,255,0.3) 30%,rgba(255,255,255,0.7) 60%,#FFFFFF 100%);pointer-events:none;z-index:7}

/* ===== SNOWFALL ===== */
.bbx-snowfall{position:absolute;top:0;left:0;width:100%;height:100%;pointer-events:none;z-index:8;overflow:hidden;transition:opacity .2s ease,visibility .2s ease}
.bbx-flake{position:absolute;top:-20px;color:#93C5FD;opacity:0;animation:snowdrift linear infinite;text-shadow:0 0 6px rgba(255,255,255,.9)}
.bbx-flake:nth-child(1){left:5%;font-size:14px;animation-duration:12s;animation-delay:0s}
.bbx-flake:nth-child(2){left:12%;font-size:10px;animation-duration:15s;animation-delay:2s}
.bbx-flake:nth-child(3){left:20%;font-size:16px;animation-duration:10s;animation-delay:1s}
.bbx-flake:nth-child(4){left:28%;font-size:12px;animation-duration:14s;animation-delay:3s}
.bbx-flake:nth-child(5){left:35%;font-size:18px;animation-duration:11s;animation-delay:0.5s}
.bbx-flake:nth-child(6){left:42%;font-size:11px;animation-duration:13s;animation-delay:4s}
.bbx-flake:nth-child(7){left:50%;font-size:14px;animation-duration:12s;animation-delay:1.5s}
.bbx-flake:nth-child(8){left:58%;font-size:13px;animation-duration:14s;animation-delay:2.5s}
.bbx-flake:nth-child(9){left:65%;font-size:17px;animation-duration:10s;animation-delay:0s}
.bbx-flake:nth-child(10){left:72%;font-size:10px;animation-duration:16s;animation-delay:3.5s}
.bbx-flake:nth-child(11){left:80%;font-size:15px;animation-duration:11s;animation-delay:1s}
.bbx-flake:nth-child(12){left:88%;font-size:12px;animation-duration:13s;animation-delay:2s}
.bbx-flake:nth-child(13){left:95%;font-size:16px;animation-duration:12s;animation-delay:4.5s}
.bbx-flake:nth-child(14){left:8%;font-size:13px;animation-duration:15s;animation-delay:5s}
.bbx-flake:nth-child(15){left:45%;font-size:11px;animation-duration:14s;animation-delay:6s}
.bbx-flake:nth-child(16){left:62%;font-size:14px;animation-duration:11s;animation-delay:3s}
.bbx-flake:nth-child(17){left:78%;font-size:18px;animation-duration:13s;animation-delay:1s}
.bbx-flake:nth-child(18){left:32%;font-size:12px;animation-duration:12s;animation-delay:2s}
.bbx-flake:nth-child(19){left:18%;font-size:15px;animation-duration:14s;animation-delay:7s}
.bbx-flake:nth-child(20){left:52%;font-size:13px;animation-duration:16s;animation-delay:8s}
.bbx-flake:nth-child(21){left:83%;font-size:11px;animation-duration:13s;animation-delay:5.5s}
.bbx-flake:nth-child(22){left:25%;font-size:16px;animation-duration:11s;animation-delay:4s}
.bbx-flake:nth-child(23){left:70%;font-size:14px;animation-duration:15s;animation-delay:6.5s}
.bbx-flake:nth-child(24){left:40%;font-size:12px;animation-duration:12s;animation-delay:3.5s}
@keyframes snowdrift{0%{transform:translateY(0) translateX(0) rotate(0deg);opacity:0}5%{opacity:.7}50%{transform:translateY(50vh) translateX(20px) rotate(180deg);opacity:.8}95%{opacity:.6}100%{transform:translateY(100vh) translateX(-15px) rotate(360deg);opacity:0}}

/* ===== MAIN CONTENT ===== */
.bbx-wrap{position:relative;z-index:10;max-width:1280px;margin:0 auto;padding:clamp(30px,6vw,60px) clamp(20px,4vw,40px) clamp(180px,22vw,280px);text-align:center}
.bbx-trust-top{position:relative;display:inline-flex;align-items:center;gap:8px;background:rgba(255,255,255,0.9);backdrop-filter:blur(10px);border:2px solid #DBEAFE;border-radius:999px;padding:18px 24px;margin-bottom:32px;box-shadow:0 4px 20px rgba(37,99,235,.1)}
.bbx-trust-rating{font-size:16px;font-weight:700;color:#111}
.bbx-trust-stars{display:flex;gap:2px}
.bbx-trust-stars svg{width:16px;height:16px;fill:#00B67A}
.bbx-trust-label{font-size:14px;font-weight:600;color:#64748B}
.bbx-title{font:900 clamp(36px,6vw,64px)/1.2 ui-sans-serif,system-ui,-apple-system,Segoe UI,Roboto,"Helvetica Neue",Arial;margin:0 0 36px;letter-spacing:-.02em;color:#111;max-width:1100px;margin-left:auto;margin-right:auto}
.bbx-title .accent{display:inline-block;background:linear-gradient(120deg,#2563EB 0%,#1D4ED8 40%,#3B82F6 70%,#60A5FA 100%);-webkit-background-clip:text;-webkit-text-fill-color:transparent;background-clip:text;animation:shimmer 6s ease-in-out infinite;background-size:200% 100%}
@keyframes shimmer{0%,100%{background-position:0% 0%}50%{background-position:100% 0%}}
.bbx-desc{max-width:820px;margin:0 auto 40px;font:400 clamp(16px,1.6vw,18px)/1.6 system-ui,-apple-system,Segoe UI,Roboto,"Helvetica Neue",Arial;color:#64748B}
.bbx-desc strong{color:#111;font-weight:700}
@media (max-width:768px){.bbx-desc{display:none}}
.bbx-cta-wrap{position:relative;display:inline-block;margin-bottom:40px}
.bbx-cta{position:relative;display:inline-flex;align-items:center;justify-content:center;padding:24px 48px;background:linear-gradient(135deg,#FF6D00 0%,#FF8C42 100%);color:#FFF;font-size:18px;font-weight:900;text-transform:uppercase;letter-spacing:1px;border-radius:16px;border:none;cursor:pointer;box-shadow:0 8px 30px rgba(255,109,0,.4);transition:all .3s cubic-bezier(.34,1.56,.64,1);text-decoration:none!important;animation:cta-glow 3s ease-in-out infinite;overflow:visible}
.bbx-cta::before{content:'';position:absolute;top:0;left:0;right:0;bottom:0;background:repeating-linear-gradient(45deg,transparent,transparent 10px,rgba(255,255,255,.1) 10px,rgba(255,255,255,.1) 20px);border-radius:16px;pointer-events:none}
.bbx-cta::after{--overflow-x:8px;content:'';position:absolute;top:-8px;left:calc(var(--overflow-x) * -1);border-image-source:url('https://assets.codepen.io/4175254/snow-cap-test-2.png');border-image-slice:calc(6 * 56 / 20) fill;border-image-width:calc(32px / 3);border-image-repeat:round;width:calc(100% + var(--overflow-x) * 2);height:32px;filter:drop-shadow(0 2px 2px rgba(0,0,0,.2));z-index:10;pointer-events:none}
@keyframes cta-glow{0%,100%{box-shadow:0 8px 30px rgba(255,109,0,.4)}50%{box-shadow:0 12px 40px rgba(255,109,0,.5)}}
.bbx-cta span.btn-text{position:relative;z-index:1}
.bbx-cta:hover{transform:translateY(-6px) scale(1.05);box-shadow:0 16px 50px rgba(255,109,0,.5)}
.bbx-cta-pointer{position:absolute;left:-120px;top:50%;width:120px;height:50px;pointer-events:none;animation:point-wiggle 2s ease-in-out infinite;transform:translateY(-50%)}
.bbx-cta-pointer path{stroke:#FF6D00;stroke-width:3;fill:none;stroke-linecap:round;stroke-linejoin:round}
@keyframes point-wiggle{0%,100%{transform:translateY(-50%) translateX(-5px)}50%{transform:translateY(-50%) translateX(5px)}}

/* Creator carousel */
.bbx-creators{position:relative;overflow:hidden;margin:40px 20px 0;padding:20px 0 30px;background:rgba(255,255,255,0.85);backdrop-filter:blur(12px);border-radius:20px;border:1px solid rgba(219,234,254,.8);box-shadow:0 8px 32px rgba(37,99,235,.08)}
.bbx-creators::before{content:'‚ú® Trusted by Top Creators! ‚ú®';display:block;text-align:center;font-size:14px;font-weight:700;color:#2563EB;text-transform:uppercase;letter-spacing:1px;margin-bottom:16px}
.bbx-creators-track{display:flex;gap:20px;animation:scroll 45s linear infinite;will-change:transform;flex-wrap:nowrap}
@keyframes scroll{0%{transform:translateX(0)}100%{transform:translateX(calc(-100% / 3))}}
.bbx-creators-track:hover{animation-play-state:paused}
.bbx-creator{flex:0 0 auto;display:flex;flex-direction:column;align-items:center;gap:10px;padding:16px 14px;background:linear-gradient(135deg,#FFF 0%,#F8FAFC 100%);border:3px solid #DBEAFE;border-radius:16px;min-width:110px;transition:all .3s cubic-bezier(.34,1.56,.64,1);box-shadow:0 4px 12px rgba(37,99,235,.08);margin-bottom:0;position:relative;overflow:visible}
.bbx-creator:hover{background:linear-gradient(135deg,#EFF6FF 0%,#DBEAFE 100%);transform:translateY(-8px) scale(1.05);border-color:#3B82F6;box-shadow:0 12px 28px rgba(37,99,235,.2)}
.bbx-creator-img-wrap{position:relative;width:64px;height:64px}
.bbx-santa-hat{position:absolute;top:-12px;left:-14px;width:40px;height:auto;z-index:10;pointer-events:none}
.bbx-santa-hat img{width:100%;height:auto;filter:drop-shadow(0 2px 2px rgba(0,0,0,.25))}
.bbx-creator-img{width:64px;height:64px;border-radius:50%;object-fit:cover;border:4px solid #3B82F6;box-shadow:0 4px 12px rgba(37,99,235,.15);background:#E5E7EB}
.bbx-creator-name{font-size:14px;font-weight:800;color:#111;text-align:center}
.bbx-creator-subs{font-size:12px;color:#64748B;display:flex;align-items:center;gap:4px;font-weight:600}
.bbx-creator-subs svg{width:14px;height:14px;fill:#3B82F6}

/* ===== MODAL STYLES ===== */
.bbx-modal-confetti{position:fixed;pointer-events:none;z-index:100001;font-size:20px;animation:modal-confetti-fall 4s ease-out forwards}
@keyframes modal-confetti-fall{0%{transform:translateY(-20px) rotate(0deg);opacity:0}10%{opacity:1}100%{transform:translateY(100vh) translateX(var(--drift,0)) rotate(360deg);opacity:0}}
.game-modal{display:none;position:fixed;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,0);z-index:100000;padding:20px;overflow-y:auto;transition:background .3s ease}
.game-modal.active{display:flex;align-items:center;justify-content:center;background:rgba(15,23,42,.9)}
.game-modal-snowfall{position:fixed;top:0;left:0;width:100%;height:100%;pointer-events:none;z-index:100000;overflow:hidden}
.game-modal-flake{position:absolute;top:-30px;color:#93C5FD;opacity:0;animation:modal-heavy-snow linear infinite;text-shadow:0 0 8px rgba(255,255,255,.9)}
@keyframes modal-heavy-snow{0%{transform:translateY(0) translateX(0) rotate(0deg);opacity:0}3%{opacity:.9}50%{transform:translateY(50vh) translateX(30px) rotate(180deg);opacity:1}97%{opacity:.8}100%{transform:translateY(105vh) translateX(-20px) rotate(360deg);opacity:0}}
.modal-content{position:relative;background:linear-gradient(180deg,#FFFFFF 0%,#F0F7FF 100%);border-radius:28px;padding:40px 40px 36px;max-width:1100px;width:100%;box-shadow:0 40px 100px rgba(0,0,0,.5),0 0 60px rgba(59,130,246,.2);animation:popIn .4s cubic-bezier(.34,1.56,.64,1);border:4px solid #60A5FA;overflow:visible;z-index:100001}
.modal-santa-hat{position:absolute;top:-50px;left:-75px;width:180px;height:auto;z-index:20;pointer-events:none;transform:rotate(-10deg)}
.modal-santa-hat img{width:100%;height:auto;filter:drop-shadow(0 6px 12px rgba(0,0,0,.5))}
.modal-frost{position:absolute;top:-2px;left:-2px;right:-2px;height:60px;background:linear-gradient(180deg,rgba(147,197,253,.3) 0%,transparent 100%);border-radius:28px 28px 0 0;pointer-events:none;z-index:1}
.modal-frost::before{content:'‚ùÑ';position:absolute;top:8px;left:20px;font-size:20px;color:#93C5FD;animation:crystal-spin 10s linear infinite}
.modal-frost::after{content:'‚úß';position:absolute;top:12px;right:60px;font-size:18px;color:#60A5FA;animation:crystal-spin 8s linear infinite reverse}
.modal-snowfall{display:none}
.modal-crystal{position:absolute;top:0;width:2px;background:linear-gradient(180deg,#93C5FD 0%,#BFDBFE 100%);z-index:1}
.modal-crystal::after{content:'‚úß';position:absolute;bottom:-16px;left:50%;transform:translateX(-50%);font-size:16px;color:#60A5FA;text-shadow:0 0 8px rgba(96,165,250,.5);animation:crystal-spin 6s linear infinite}
.modal-crystal:nth-child(1){left:8%;height:30px}.modal-crystal:nth-child(1)::after{animation-delay:0s}
.modal-crystal:nth-child(2){right:8%;height:35px}.modal-crystal:nth-child(2)::after{content:'‚ùÑ';font-size:14px;animation-delay:3s}
@keyframes popIn{from{opacity:0;transform:scale(.88) translateY(40px)}to{opacity:1;transform:scale(1) translateY(0)}}
.modal-header{display:flex;align-items:center;justify-content:center;padding:0 0 28px;border-bottom:2px solid #DBEAFE;margin-bottom:28px;position:relative;z-index:2}
.modal-title{margin:0;font-weight:900;letter-spacing:-.02em;font-size:42px;text-align:center;display:inline-block;background:linear-gradient(120deg,#2563EB 0%,#3B82F6 40%,#60A5FA 70%,#93C5FD 100%);-webkit-background-clip:text;-webkit-text-fill-color:transparent;background-clip:text;animation:shimmer 6s ease-in-out infinite;background-size:200% 100%}
.modal-title::before{content:'‚ú® '}
.modal-title::after{content:' ‚ú®'}
.modal-close{display:grid;place-items:center;width:48px;height:48px;border-radius:14px;border:2px solid #3B82F6;background:#FFF;cursor:pointer;color:#3B82F6;transition:all .2s ease;position:absolute;right:-24px;top:-24px;z-index:3;box-shadow:0 4px 12px rgba(0,0,0,.1)}
.modal-close:hover{background:#3B82F6;color:#FFF;transform:rotate(90deg)}
.modal-close svg{width:24px;height:24px;stroke:currentColor;fill:none}

/* ===== GAME SECTIONS ===== */
.modal-section{position:relative;z-index:2;margin-bottom:28px}
.modal-section:last-child{margin-bottom:0}
.section-header{display:flex;align-items:center;gap:12px;margin-bottom:18px}
.section-title{font-size:18px;font-weight:800;color:#1E293B;text-transform:uppercase;letter-spacing:1px;display:flex;align-items:center;gap:8px}
.section-title-icon{font-size:22px}
.section-line{flex:1;height:2px;background:linear-gradient(90deg,#DBEAFE 0%,transparent 100%);border-radius:2px}
.section-badge{background:linear-gradient(135deg,#3B82F6,#60A5FA);color:#FFF;padding:6px 14px;border-radius:20px;font-size:11px;font-weight:700;text-transform:uppercase;letter-spacing:.5px;box-shadow:0 4px 12px rgba(59,130,246,.3)}

/* Games Grid */
.modal-grid{display:grid;grid-template-columns:repeat(4,1fr);gap:16px;position:relative;z-index:2}
.game-option{position:relative;display:block;border-radius:18px;overflow:hidden;background:#FFF;box-shadow:0 4px 16px rgba(17,17,17,.08);border:3px solid #E2E8F0;color:#111;text-decoration:none!important;transition:all .3s ease}
